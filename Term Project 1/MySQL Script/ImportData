select * from restaurants;


-- Import 4th file, restaurant_cuisines
create table restaurant_cuisines
(Restaurant_ID varchar(32) NOT NULL,
Cuisine varchar(32),
FOREIGN KEY (Restaurant_ID) REFERENCES restaurants (Restaurant_ID));

LOAD DATA INFILE 'C:/ProgramData/MySQL/MySQL Server 8.0/Uploads/restaurant_cuisines.csv' 
INTO TABLE restaurant_cuisines 
FIELDS TERMINATED BY ',' 
LINES TERMINATED BY '\r\n'
IGNORE 1 LINES ;

select * from restaurant_cuisines;

-- Import 5th file, ratings
create table ratings
(Consumer_ID varchar(32) NOT NULL,
Restaurant_ID varchar(32) NOT NULL,
Overall_Rating INTEGER,
Food_Rating INTEGER,
Service_Rating INTEGER,
FOREIGN KEY (Consumer_ID) REFERENCES consumers (Consumer_ID),
FOREIGN KEY (Restaurant_ID) REFERENCES restaurants (Restaurant_ID));

LOAD DATA INFILE 'C:/ProgramData/MySQL/MySQL Server 8.0/Uploads/ratings.csv' 
INTO TABLE ratings
FIELDS TERMINATED BY ',' 
LINES TERMINATED BY '\r\n'
IGNORE 1 LINES ;


select * from ratings;

-- Get some basic information from tables
select Price, Count(restaurant_id) from restaurants 
		group by Price order by Count(restaurant_id) desc;
        -- 130 restaurants in total
select Cuisine, Count(restaurant_id) from restaurant_cuisines 
		group by Cuisine order by Count(restaurant_id) desc;
        -- Main cuisine is Mexian, then Bar, Cafeteria, Fast Food, Brewery, etc.
select budget, Count(consumer_id) from consumers 
		group by budget order by Count(consumer_id) desc;
        -- 138 consumers in total: High budget 5, Medium 91, Low 35, and 7 empty
select Preferred_Cuisine, count(Preferred_Cuisine) from consumer_preferences 
		group by Preferred_Cuisine order by count(Preferred_Cuisine) desc;
        -- Mexican is most popular preference, then American, Cafeteria, Pizzeria, etc.
select AVG(Overall_Rating) as avg_overall_rating from ratings 
		order by avg_overall_rating desc;
		-- Avg score is 1.1998. In rating, 0=  Unsatisfactory, 1 = Satisfactory, 2 = Highly Satisfactory
select Restaurant_ID, AVG(Overall_Rating) as avg_overall_rating, count(Restaurant_ID) as Number_of_rating
		from ratings group by Restaurant_ID order by avg_overall_rating desc;
        -- 3 restaurants are top-ranking but have smaller number of rating
