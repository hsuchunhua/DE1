-- Create schema
DROP SCHEMA IF EXISTS termproject1;
create schema termproject1; 
use termproject1;

-- Import data, 5 files
SHOW VARIABLES LIKE "secure_file_priv";
SHOW VARIABLES LIKE "local_infile";
set global local_infile=1;

-- Import 1st file, consumers
create table consumers
(Consumer_ID varchar(32) NOT NULL,
City varchar(32),
State varchar(32),
Country varchar(32),
Latitude INTEGER,
Longitude INTEGER,
Smoker varchar(16),
Drink_Level varchar(32),
Transportation_Method varchar(32),
Marital_Status varchar(32),
Children varchar(32),
Age integer,
Occupation varchar(32),
Budget varchar(16),
PRIMARY KEY (Consumer_ID));

LOAD DATA INFILE 'C:/ProgramData/MySQL/MySQL Server 8.0/Uploads/consumers.csv' 
INTO TABLE consumers
FIELDS TERMINATED BY ',' 
LINES TERMINATED BY '\r\n'
IGNORE 1 LINES ;

select * from consumers;

-- Import 2nd file, consumer_preferences
create table consumer_preferences
(Consumer_ID varchar(32) NOT NULL,
Preferred_Cuisine varchar(32),
FOREIGN KEY (Consumer_ID) REFERENCES consumers (Consumer_ID));

LOAD DATA INFILE 'C:/ProgramData/MySQL/MySQL Server 8.0/Uploads/consumer_preferences.csv' 
INTO TABLE consumer_preferences 
FIELDS TERMINATED BY ',' 
LINES TERMINATED BY '\r\n'
IGNORE 1 LINES ;

select * from consumer_preferences;

-- Import 3rd file, restaurants
CREATE TABLE restaurants(
   Restaurant_ID   varchar(32)  NOT NULL PRIMARY KEY 
  ,Name            VARCHAR(52) NOT NULL
  ,City            VARCHAR(15) NOT NULL
  ,State           VARCHAR(15) NOT NULL
  ,Country         VARCHAR(6) NOT NULL
  ,Zip_Code        INTEGER 
  ,Latitude        NUMERIC(10,7) NOT NULL
  ,Longitude       NUMERIC(12,7) NOT NULL
  ,Alcohol_Service VARCHAR(11) NOT NULL
  ,Smoking_Allowed VARCHAR(15) NOT NULL
  ,Price           VARCHAR(6) NOT NULL
  ,Franchise       VARCHAR(3) NOT NULL
  ,Area            VARCHAR(6) NOT NULL
  ,Parking         VARCHAR(6) NOT NULL
);

